<h1>New Asset</h1>

<%= error_messages_for :asset %>

<% form_for(:asset, :url => assets_path, 
                    :html => { :multipart => true }) do |f| -%>
  <p>
    <label for="asset">Upload An Image:</label>
    <%= f.file_field :uploaded_data %>
  </p>
  <p>
    <%= submit_tag 'Create' %>
  </p>
<% end -%>